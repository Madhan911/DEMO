[32m+[m[32mclass Home:[m
[32m+[m[32m    text_by_name_xpath = "//input[@type='text'][@class='form-control']"[m
[32m+[m[32m    dropdown_by_specialty_xpath = "//select[@name='select'][@aria-label='maaspecialityselectlabel']"[m
[32m+[m[32m    btn_by_specialty_xpath = "//div[@class='button-div hidden-xs']"[m
[32m+[m
[32m+[m[32m    def __init__(self, driver):[m
[32m+[m[32m        self.driver = driver[m
[32m+[m
[32m+[m[32m    def by_Specialty(self, specialty):[m
[32m+[m[32m        self.driver.find_element_by_xpath(self.dropdown_by_specialty_xpath).clear()[m
[32m+[m[32m        self.driver.find_element_by_xpath(self.dropdown_by_specialty_xpath).sendkeys(specialty)[m
[32m+[m
[32m+[m[32m    def next_Button(self):[m
[32m+[m[32m        self.driver.find_element_by_xpath(self.btn_by_specialty_xpath).click()[m
