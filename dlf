[32m+[m[32mimport configparser[m
[32m+[m
[32m+[m[32mconfig = configparser.RawConfigParser()[m
[32m+[m[32mconfig.read("./Configurations/config.ini")[m
[32m+[m
[32m+[m
[32m+[m[32mclass readConfig:[m
[32m+[m[32m    @staticmethod[m
[32m+[m[32m    def getApllicationURL():[m
[32m+[m[32m        url = config.get('common info', 'baseURL')[m
[32m+[m[32m        return url[m
