[32m+[m[32mimport pytest[m
[32m+[m[32mfrom selenium import webdriver[m
[32m+[m[32mfrom PageObjects.PagetitleHome import Home[m
[32m+[m[32mfrom Utilities.readProperties import readConfig[m
[32m+[m
[32m+[m
[32m+[m[32m@pytest.mark.usefixtures("setup1")[m
[32m+[m[32mclass Test_Appointment(BasicTest):[m
[32m+[m
[32m+[m[32m    baseURL = readConfig.getApllicationURL()[m
[32m+[m
[32m+[m[32m    def test_homepageTitel(self, setup):[m
[32m+[m
[32m+[m[32m        # self.logger.info("****************Test_001_Login*************  ")[m
[32m+[m[32m        # self.logger.info("**************** Verifying home page title *************  ")[m
[32m+[m[32m        log = self.getLogger()[m
[32m+[m[32m        log.info("Launching amazon website")[m
[32m+[m[32m        self.driver.get(baseURL)[m
[32m+[m[32m        act_title = self.driver.title[m
[32m+[m
[32m+[m[32m        if act_title == "Request an Appointment | Mount Sinai - New York":[m
[32m+[m[32m            assert True[m
[32m+[m[32m            self.driver.close()[m
[32m+[m[32m            # self.logger.info("****************Homepage title test is passed *************  ")[m
[32m+[m[32m        else:[m
[32m+[m[32m            self.driver.save_screenshot(".\\Screenshots\\" + "test_homepageTitle.png")[m
[32m+[m[32m            self.driver.close()[m
[32m+[m[32m            # self.logger.error("****************Homepage title test is failed *************  ")[m
[32m+[m[32m            assert False[m
